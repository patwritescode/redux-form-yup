{
    "name": "redux-form-yup",
    "version": "0.0.5",
    "description": "Automatic async validation for redux-form using yup schemas.",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "license": "MIT",
    "keywords": [
        "yup",
        "redux-form",
        "validation"
    ],
    "repository": "patwritescode/redux-form-yup",
    "scripts": {
        "build": "rollup -c",
        "lint": "tslint --project . --fix",
        "test": "jest",
        "prepublish": "npm test && npm run lint && npm run build"
    },
    "dependencies": {
        "object-set": "^1.0.1",
        "yup": "^0.25.1"
    },
    "devDependencies": {
        "@types/jest": "^23.1.0",
        "@types/yup": "^0.24.6",
        "rollup-plugin-commonjs": "^9.1.3",
        "jest": "^23.1.0",
        "rollup": "^0.60.7",
        "rollup-plugin-typescript2": "^0.15.0",
        "ts-jest": "^22.4.6",
        "tslint": "^5.10.0",
        "typescript": "^2.9.2"
    },
    "author": {
        "name": "Patrick Dunn",
        "email": "patnpm@outlook.com",
        "url": "http://patrickdunn.org"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        },
        "globals": {
            "ts-jest": {
                "tsConfigFile": "tsconfig.json"
            }
        },
        "testMatch": [
            "**/__tests__/*.+(ts|tsx|js)"
        ]
    }
}
